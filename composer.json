{
    "name": "remotelyliving/php-cache-adapter",
    "description": "A lightweight PSR6 / PSR16 cache adapter library for common caching scenarios",
    "keywords": ["cache adapter", "cache", "PSR-16", "PSR-6", "Redis", "Memcache", "APCu"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Thomas",
            "email": "christian.h.thomas@me.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-filter": "*",
        "ext-mbstring": "*",
        "psr/simple-cache": "^1.0",
        "psr/cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "maglnet/composer-require-checker": "^2.0",
        "squizlabs/php_codesniffer": "^3.3",
        "php-coveralls/php-coveralls": "^2.2",
        "vimeo/psalm": "^3.7",
        "phpstan/phpstan": "^0.12",
        "psy/psysh": "^0.10.4"
    },
    "autoload": {
        "psr-4": {
            "RemotelyLiving\\PHPCacheAdapter\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RemotelyLiving\\PHPCacheAdapter\\Tests\\" : "tests/"
        }
    }
}
